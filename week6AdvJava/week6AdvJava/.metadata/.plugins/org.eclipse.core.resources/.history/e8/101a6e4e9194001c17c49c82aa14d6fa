package com.nagarro.weekSix;

import java.util.Scanner;

public class Driver {
	String gender, sortBy, color, size;
	@SuppressWarnings("resource")
	Scanner sc = new Scanner("System.in");
	
	gender=sc.nextLine();
	System.out.println("Enter color: ");
	color=sc.nextLine();
	System.out.println("Enter Size(S|M|L|XL|XXL): ");
	size=sc.nextLine();
	System.out.println("Enter Sorting Preference(P|R|P+R): ");
	sortBy=sc.nextLine();
	scriptClass obj = new scriptClass(gender, color, size, sortBy);
	obj.GetData();
	obj.printList();
}
